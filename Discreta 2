n = input("Forneça um número de registro ISBN13. Será retornado seu digito de verificação.")
n = [int(i) for i in str(n)]
s =((1 * n[0]) + (3 * n[1]) +(1 * n[2]) + (3 * n[3]) + (1 * n[4]) + (3 * n[5]) +(1 * n[6]) + (3 * n[7]) +(1 * n[8]) + (3 * n[9]) +(1 * n[10]) + (3 * n[11])) % 10
print(s)
print(10 - s)
